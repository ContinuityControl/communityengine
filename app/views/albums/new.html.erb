<div class='yui-b'>
    <%= box do %>
  <h3>Tips</h3>
  <p>
        <%= :album_tip.l :community_name => AppConfig.community_name %>

  </p>
  <% end %>

    <%= box :class => 'alt' do %>
    <%=  link_to :back_to_photo_manager.l, user_photo_manager_index_path(current_user) %>

  <% end %>

</div>
<div id='yui-main'>
  <div class='yui-b'>
        <%= box :id => "single_upload", :class => ".yui-b" do %>
    <h3>    <%= :new_album.l %>
</h3>
        <%=  form_for(@album, :url => user_albums_path(current_user.id), :html => {:class => "MainForm"}) do |f| %>
        <%=  render :partial => 'form', :locals => {:f => f, :button1 => :create_and_add_photos.l, :button2 => :create_album.l } %>

    <% end %>

    <% end %>

  </div>
</div>
