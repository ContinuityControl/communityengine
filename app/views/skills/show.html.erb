<div class='yui-b'>
    <%=  render :partial => "skills/sidebar_menu" %>

</div>
<div id='yui-main'>
    <%= box :class => "yui-b user_list" do %>
  <h3>
        <%=  AppConfig.community_name %>

        <%= :pros_skilled_in.l %>

        <%=  @skill.name %>

  </h3>
    <%=  content_tag :h2, :sorry_we_dont_have_any_pros_in_this_skill_category_yet.l if @skill.users.empty? %>

    <%=  render :partial => "users/user", :collection => @users %>

  <br class='clear' />
  <div class='pagination'>  <%= paginating_links @users if @users.total_pages > 1 %>
</div>
  <% end %>

</div>
